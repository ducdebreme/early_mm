        </div>

        <div class="col-md-6">

          <%= partial("text/event-detail-description") %>
 
        </div>
        <div class="col-md-2 omega">
          <!-- tags -->
          <a class="label label-primary" href="#">All levels</a>
          <a class="label label-primary" href="#">Baroque</a>
          <a class="label label-primary" href="#">Ongoing classes</a>
        </div>
      </div>
      <!-- END MAIN CONTENT ROW -->

      <!-- BEGIN MIDDLE ROW : VENUE -->
      <!-- include a partial with data -->
      <%= partial( :address_with_map, :locals => { 
        :name => "Tanzstudio Manhardt ", :street => "Neubaugasse 38", :place => "Wien", :country => "Austria"
      })  %>      
      <!-- END MIDDLE ROW -->


      <!-- BEGIN ROW : INVOLVED -->
      <!-- translate here depending on the event type -->
      <div class="row section">
        <h3>Teachers</h3>
        <div class="col-md-4">
          <!-- INVOLVED -->
          <!-- include a partial with data -->
          <%= partial( :address_short, :locals => { 
            :name => "Pia Brocza", :street => "Hausstrasse 3", :place => "Wien", :country => "Austria"
          })  %>

        </div>
        <div class="col-md-4">
          <!-- INVOLVED -->
          <!-- include a partial with data -->
          <%= partial( :address_short, :locals => { 
            :name => "Bernd Niedecken", :street => "Innsbrucker Str. 97", :place => "Freiburg", :country => "Germany"
          })  %>
        </div>
      </div>


      <!-- BEGIN ROW : CONTACT -->
